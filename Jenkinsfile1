pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World Jenkins'
            }
        }
        stage('Build') {
            steps {
                echo 'Building has been successfully'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying has been successfully'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
         }
         }

